{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
		"no-console": "off",
		"linebreak-style": "off",
		"eol-last": "off",
		"indent": "off",
		"space-before-function-paren": "off",
		"spaced-comment": "off"
    }
}
